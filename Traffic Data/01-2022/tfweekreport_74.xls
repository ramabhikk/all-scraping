					var closeSideMenu = function()
					{
						$('.sidebar-nav').removeClass('nav-expanded');
						$('.nav-expander').removeClass('nav-expander-expanded');
						localStorage.setItem('c2_showmenu', false);
						sideMenuOpen = false;
					};
					
					var openSideMenu = function()
					{
						$('.sidebar-nav').addClass('nav-expanded');
						$('.nav-expander').addClass('nav-expander-expanded');
						localStorage.setItem('c2_showmenu', true);
						sideMenuOpen = true;
					};

					$('#nav-expander').on('click',function(e)
					{
						e.preventDefault();
						sideMenuOpen ? closeSideMenu() : openSideMenu();
					});
					
					$('#nav-close').on('click',function(e)
					{
						e.preventDefault();
						closeSideMenu();
					});

					// Set the initial menu state
					if($('#nav-expander') && $('#nav-expander').length > 0)
					{
						sideMenuOpen ? openSideMenu() : closeSideMenu();
					}
					
					var footerOpen = (localStorage.getItem('c2_showfooter') === 'true');
					
					var closeFooter = function()
					{
						$('#footer').css('bottom', '-' + ($('#footer').height()) + 'px').css('transition', 'bottom 0.3s ease-in-out 0s');
						$('#footer-expander').html('<span class="glyphicon glyphicon-chevron-up">');
						setTimeout(function()
						{	
							$('#footer div').hide();
						}, 100);
						localStorage.setItem('c2_showfooter', false);
						footerOpen = false;
					};
					
					var openFooter = function()
					{
						$('#footer div').show();
						$('#footer').css('bottom', '0px').css('transition', 'bottom 0.3s ease-in-out 0s');
						$('#footer-expander').html('<span class="glyphicon glyphicon-chevron-down">');
						localStorage.setItem('c2_showfooter', true);
						footerOpen = true;
					};

					$('#footer-expander').on('click',function(e)
					{
						e.preventDefault();
						footerOpen ? closeFooter() : openFooter();
					});

					// Set the initial footer state
					if($('#footer-expander') && $('#footer-expander').length > 0)
					{
						footerOpen ? openFooter() : closeFooter();
					}
					
					
